{
  "name": "vue-ssr-xiaoxx",
  "version": "1.0.0",
  "description": "vue + ssr ",
  "main": "index.js",
  "scripts": {
    "client": "webpack --config src/config/webpack.client.config.js && node src/server.js",
    "server": "webpack --config src/config/webpack.server.config.js && node src/server.js",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "dev": "rimraf build && webpack --config src/config/webpack.client.config.js && webpack --config src/config/webpack.server.config.js && node src/server.js",
    "clean": "rimraf build",
    "build:client": "cross-env NODE_ENV=production webpack --config src/config/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config src/config/webpack.server.config.js",
    "build": "rimraf build && npm run build:client && npm run build:server && node src/server.js"
  },
  "author": "xiaoxx",
  "license": "ISC",

  "devDependencies": {
    "chokidar": "^2.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "http-proxy-middleware": "^0.19.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/babel-preset-app": "^3.4.1",
    "autoprefixer": "^9.1.3",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-vue-app": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "vue": "^2.6.7",
    "vue-loader": "^15.6.4",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.7",
    "vuex": "^3.1.0",
